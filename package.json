{
  "name": "analyze",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --mode dev",
    "prod": "vue-cli-service serve --mode pord"
  },
  "dependencies": {
    "axios": "^0.21.1",
